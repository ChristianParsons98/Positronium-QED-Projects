Cell["mathForm", "Subsection",
 CellChangeTimes->{{3.608294231752681*^9, 3.6082942394112263`*^9}}],

Cell["\<\
mathForm turns the encoded expressions of the form {{n1, {a1*p0 + b1, ...}}, \
{n2, {a2*p0 + b2, ...}}, ...} into a standard mathematica form n1/((a1*p0 + \
b1) ...) + n2/((a2*p0 + b2) ...) + ....\
\>", "Text",
 CellChangeTimes->{{3.603546761870164*^9, 3.603546846265519*^9}, {
  3.60829426090357*^9, 3.6082942790703278`*^9}, {3.61227949742031*^9, 
  3.612279503555773*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"mathForm", "[", "dat_", "]"}], ":=", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"dat", "[", 
       RowBox[{"[", 
        RowBox[{"ind", ",", "1"}], "]"}], "]"}], "/", 
      RowBox[{"Product", "[", 
       RowBox[{
        RowBox[{"dat", "[", 
         RowBox[{"[", 
          RowBox[{"ind", ",", "2", ",", "k"}], "]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "1", ",", 
          RowBox[{"Length", "[", 
           RowBox[{"dat", "[", 
            RowBox[{"[", 
             RowBox[{"ind", ",", "2"}], "]"}], "]"}], "]"}]}], "}"}]}], 
       "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"ind", ",", "1", ",", 
       RowBox[{"Length", "[", "dat", "]"}]}], "}"}]}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.603488113038659*^9, 3.6034881670536003`*^9}, {
  3.603488222380044*^9, 3.60348830752295*^9}}],

