Cell[CellGroupData[{

Cell["CauchyIntegrate", "Subsection",
 CellChangeTimes->{{3.608569957329855*^9, 3.6085699677341433`*^9}}],

Cell["\<\
CauchyIntegrate performs the poles integration routine on a sum of terms of \
the form n1/((a1 p0 + b1) ...) + n2/((a2 p0 + b2) ...).  The integrand is \
encoded as a list of lists: {{n1,{a1*p0+b1,...}},{n2,{a2*p0+b2,...}}}.\
\>", "Text",
 CellChangeTimes->{{3.603546615282077*^9, 3.6035467509149446`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"CauchyIntegrate", "[", 
   RowBox[{"integrand_", ",", "var_", ",", "sign_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"out", ",", "ind"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"out", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"cauchy", "[", 
           RowBox[{
            RowBox[{"integrand", "[", 
             RowBox[{"[", "ind", "]"}], "]"}], ",", "var", ",", "sign"}], 
           "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"ind", ",", "1", ",", 
            RowBox[{"Length", "[", "integrand", "]"}]}], "}"}]}], "]"}], ",", 
        "1"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"out", "\[NotEqual]", 
        RowBox[{"{", 
         RowBox[{"{", 
          RowBox[{"0", ",", 
           RowBox[{"{", "}"}]}], "}"}], "}"}]}], ",", 
       RowBox[{"out", "=", 
        RowBox[{"Delete", "[", 
         RowBox[{"out", ",", 
          RowBox[{"Position", "[", 
           RowBox[{"out", ",", 
            RowBox[{"{", 
             RowBox[{"0", ",", 
              RowBox[{"{", "}"}]}], "}"}]}], "]"}]}], "]"}]}], ",", 
       RowBox[{"out", "=", "out"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "out", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.603474102333709*^9, 3.6034741247331457`*^9}, 
   3.603474595437706*^9}]
}, Closed]],

