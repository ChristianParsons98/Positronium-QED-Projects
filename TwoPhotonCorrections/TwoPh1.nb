Cell["CauchyIntegrateX", "Subsection",
 CellChangeTimes->{{3.608569957329855*^9, 3.6085699677341433`*^9}, 
   3.612432090005509*^9}],

Cell["\<\
CauchyIntegrateX performs the poles integration routine on a sum of terms of \
the form num1/((a1 p0 + b1 + iim eps1)^n1 ...) + num2/((a2 p0 + b2 + iim \
eps2)^n2 ...).  The integrand is encoded as a list of lists: \
{{num1,{{a1*p0+b1+iim eps1,n1},...}},{num2,{{a2*p0+b2+iim eps2,n2},...}}}.\
\>", "Text",
 CellChangeTimes->{{3.603546615282077*^9, 3.6035467509149446`*^9}, {
  3.612432104182705*^9, 3.612432219305791*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"CauchyIntegrateX", "[", 
   RowBox[{"integrand_", ",", "var_", ",", "sign_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"out", ",", "ind"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"out", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"cauchyX", "[", 
           RowBox[{
            RowBox[{"integrand", "[", 
             RowBox[{"[", "ind", "]"}], "]"}], ",", "var", ",", "sign"}], 
           "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"ind", ",", "1", ",", 
            RowBox[{"Length", "[", "integrand", "]"}]}], "}"}]}], "]"}], ",", 
        "1"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"out", "\[NotEqual]", 
        RowBox[{"{", 
         RowBox[{"{", 
          RowBox[{"0", ",", 
           RowBox[{"{", "}"}]}], "}"}], "}"}]}], ",", 
       RowBox[{"out", "=", 
        RowBox[{"Delete", "[", 
         RowBox[{"out", ",", 
          RowBox[{"Position", "[", 
           RowBox[{"out", ",", 
            RowBox[{"{", 
             RowBox[{"0", ",", 
              RowBox[{"{", "}"}]}], "}"}]}], "]"}]}], "]"}]}], ",", 
       RowBox[{"out", "=", "out"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"out", "=", 
      RowBox[{"removeZerosX", "[", "out", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "out", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
