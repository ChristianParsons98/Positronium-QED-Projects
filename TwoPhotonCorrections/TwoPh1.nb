       RowBox[{"{", 
        RowBox[{"{", 
         RowBox[{"0", ",", 
          RowBox[{"{", "}"}]}], "}"}], "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"D", "[", 
             RowBox[{
              RowBox[{"dat", "[", 
               RowBox[{"[", 
                RowBox[{"2", ",", "ind"}], "]"}], "]"}], ",", "var"}], "]"}], 
            "\[NotEqual]", "0"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"Switch", "[", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"Assuming", "[", 
               RowBox[{"epsAssump", ",", 
                RowBox[{"Refine", "[", 
                 RowBox[{"Sign", "[", 
                  RowBox[{
                   RowBox[{"D", "[", 
                    RowBox[{
                    RowBox[{"dat", "[", 
                    RowBox[{"[", 
                    RowBox[{"2", ",", "ind"}], "]"}], "]"}], ",", "var"}], 
                    "]"}], "*", 
                   RowBox[{"D", "[", 
                    RowBox[{
                    RowBox[{"dat", "[", 
                    RowBox[{"[", 
                    RowBox[{"2", ",", "ind"}], "]"}], "]"}], ",", "iim"}], 
                    "]"}], "*", "sign"}], "]"}], "]"}]}], "]"}], 
              "\[IndentingNewLine]", ",", 
              RowBox[{"+", "1"}], ",", 
              RowBox[{"out", "=", "out"}], "\[IndentingNewLine]", ",", 
              RowBox[{"-", "1"}], ",", 
              RowBox[{"out", "=", 
               RowBox[{"Join", "[", 
                RowBox[{"out", ",", 
                 RowBox[{"{", 
                  RowBox[{"{", 
                   RowBox[{
                    RowBox[{
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"sign", "*", 
                    RowBox[{
                    RowBox[{"dat", "[", 
                    RowBox[{"[", "1", "]"}], "]"}], "/", 
                    RowBox[{"D", "[", 
                    RowBox[{
                    RowBox[{"dat", "[", 
                    RowBox[{"[", 
                    RowBox[{"2", ",", "ind"}], "]"}], "]"}], ",", "var"}], 
                    "]"}]}]}], ")"}], "/.", 
                    RowBox[{"First", "[", 
