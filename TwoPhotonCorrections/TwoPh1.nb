                    RowBox[{"dat", "[", 
                    RowBox[{"[", "2", "]"}], "]"}], ",", "ind"}], "]"}], "/.", 
                    RowBox[{"First", "[", 
                    RowBox[{"Solve", "[", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"dat", "[", 
                    RowBox[{"[", 
                    RowBox[{"2", ",", "ind"}], "]"}], "]"}], "\[Equal]", 
                    "0"}], ",", "var"}], "]"}], "]"}]}]}], "}"}], "}"}]}], 
                "]"}]}], "\[IndentingNewLine]", ",", "_", ",", 
              RowBox[{"Print", "[", 
               RowBox[{"\"\<sign of \>\"", ",", 
                RowBox[{"D", "[", 
                 RowBox[{
                  RowBox[{"dat", "[", 
                   RowBox[{"[", 
                    RowBox[{"2", ",", "ind"}], "]"}], "]"}], ",", "iim"}], 
                 "]"}], ",", "\"\< not determined\>\""}], "]"}]}], "]"}], 
            ";"}], "\[IndentingNewLine]", ",", 
           RowBox[{"out", "=", "out"}]}], "]"}], ";"}], "\[IndentingNewLine]",
         ",", 
        RowBox[{"{", 
